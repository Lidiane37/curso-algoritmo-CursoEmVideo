algoritmo "Conversor"
// Função :
// Autor :
// Data : 14/03/2024
// Seção de Declarações
var
R,D:Real
C, Q:Inteiro
inicio
// Seção de Comandos
C<-1
Escreval("Quantas vezes você quer converter:")
leia(Q)

Enquanto(C <= Q)Faca


   Escreva("Qual o valor em R$? ")
   Leia(R)
   D <- R/2,20
   Escreval("O valor convertido em  U$$? ", D:5:2)
C<-C+1

FimEnquanto

   

fimalgoritmo
